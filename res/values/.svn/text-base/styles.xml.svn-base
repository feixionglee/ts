<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="DefaultTheme">
	 	<item name="android:windowBackground">@drawable/theme_default_listbackground</item> 
		<item name="ListViewDivider">@drawable/theme_default_divider</item>
		<item name="ListViewBackground">@drawable/theme_default_listbackground</item>
		<item name="ListViewBackgroundAt">@drawable/theme_default_listbackgroundat</item>
		<item name="ListViewBackgroundMy">@drawable/theme_default_listbackgroundmy</item>
		<item name="ListViewBackgroundSelect">@drawable/theme_default_listbackgroundselect</item>
		<item name="TextViewColor">#ff333333</item>
		<item name="TextViewColorSelect">#ff333333</item>
	</style>

	<style name="LightTheme">
		<item name="android:windowBackground">@drawable/theme_light_listbackground</item>
		<item name="ListViewDivider">@drawable/theme_default_divider</item>
		<item name="ListViewBackground">@drawable/theme_light_listbackground</item>
		<item name="ListViewBackgroundAt">@drawable/theme_light_listbackgroundat</item>
		<item name="ListViewBackgroundMy">@drawable/theme_light_listbackgroundmy</item>
		<item name="ListViewBackgroundSelect">@drawable/theme_light_listbackgroundselect</item>
		<item name="TextViewColor">#ff595959</item>
		<item name="TextViewColorSelect">#ff595959</item>
	</style>
	
	<style name="DarkTheme">
		<item name="android:windowBackground">@drawable/theme_dark_listbackground</item> 
		<item name="android:popupBackground">@drawable/theme_dark_listbackground</item> 
  		<item name="ListViewDivider">@drawable/theme_dark_divider</item>
		<item name="ListViewBackground">@drawable/theme_dark_listbackground</item>
		<item name="ListViewBackgroundAt">@drawable/theme_dark_listbackgroundat</item>
		<item name="ListViewBackgroundMy">@drawable/theme_dark_listbackgroundmy</item>
		<item name="ListViewBackgroundSelect">@drawable/theme_dark_listbackgroundselect</item>
		<item name="TextViewColor">#ffdadbde</item>
		<item name="TextViewColorSelect">#ffdadbde</item>
	</style>
	
</resources>

